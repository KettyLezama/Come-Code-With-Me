<!DOCTYPE html>
<html class="has-navbar-fixed-top">
<head>
  <title>ComeCodeWithMe</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= csrf_meta_tags %>
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
</head>
<body>
  <nav class="navbar is-fixed-top" role="navigation" aria-label="main navigation">
    <div class="navbar-brand">
      <%- if current_user -%>
        <% hrefPath = "/dashboard" %>
      <%- else -%>
        <% hrefPath = "/" %>
      <%- end -%>
      <a class="navbar-item" href=<%= hrefPath %>>
        <img src="data:image/svg+xml;base64," width="50" height="50"/>
          come code with me
      </a>

      <a role="button" class="navbar-burger burger" aria-label="menu" aria-expanded="false" data-target="navbarBasicExample">
        <span aria-hidden="true"></span>
        <span aria-hidden="true"></span>
        <span aria-hidden="true"></span>
      </a>
    </div>

    <div id="navbarBasicExample" class="navbar-menu">
      <div class="navbar-end">
        <div class="navbar-item">
          <div class="buttons">
            <%- if current_user -%>
              <%= link_to 'Sign Out', destroy_user_session_path, class:"button is-primary", method: :delete %>
            <%- else -%>
              <%= link_to 'Sign Up', new_user_registration_path, class: "button is-primary" %>
              <%= link_to 'Log in', new_user_session_path, class: "button is-light" %>
            <%- end -%>
          </div>
        </div>
      </div>
    </div>
  </nav>

  <%- flash.each do |key, value| -%>
    <div class="flash flash-<%= key %>">
      <%= value %>
    </div>
  <%- end -%>

  <%= yield %>

  <%= javascript_include_tag 'application' %>
  
  <%- if Rails.env.development? || Rails.env.test? -%> 
    <%= javascript_tag do %>
      window.railsEnv = '<%= Rails.env %>'
    <%- end -%>
  <%- end -%>
  <%= javascript_pack_tag 'application' %>
  
  <%= yield :extra_footer %>
</body>
</html>
